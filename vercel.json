{
    "version": 2,
    "builds": [
        {
            "src": "./main.py",
            "use": "@vercel/python",
            "config": {
                "excludeFiles": "{public,.next,pages,images,_next,gen}/**"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}