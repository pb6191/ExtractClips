{
    "version": 2,
    "builds": [
        {
            "src": "./main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/**": {
            "excludeFiles": "{public,.next,pages,images,_next,gen}/**"
        }
    }
}